<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">30%</item>
    </style>

    <style name="cornersMainBottomBar" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSize">32dp</item>
    </style>

    <style name="cornersScrollCard" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSize">32dp</item>
    </style>

    <style name="customRoundedImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>

    <style name="buttonDelete" parent="Widget.Material3.Button.OutlinedButton">
        <item name="android:textColor">@color/error_color</item>
        <item name="strokeColor">@color/error_color</item>
    </style>

    <style name="CustomTextInputStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeWidth">2dp</item>
        <item name="boxBackgroundColor">@color/secondary_color</item>
        <item name="android:hint">@color/secondary_color</item>
    </style>

</resources>